    "react-intersection-observer-hook": "^4.0.1",
    "react-redux": "9.2.0",
    "react-syntax-highlighter": "^16.1.0",
    "react-toastify": "^11.0.5",
    "sanitize-html": "^2.17.0",
    "sass": "1.94.1",
