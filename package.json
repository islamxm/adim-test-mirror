{
  "name": "client-application",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "type-check": "tsc --noEmit",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "format:check": "prettier --check \"src/**/*.{ts,tsx,md,json}\"",
    "format:fix": "prettier --write \"src/**/*.{ts,tsx,md,json}\"",
    "check": "npm run type-check && npm run lint && npm run format:check",
    "fix": "npm run format:fix && npm run lint:fix",
    "prepare": "husky && husky install",
    "analyze": "npx next build --webpack",
    "docker:up": "docker-compose up --build -d",
    "docker:down": "docker-compose down",
    "docker:logs": "docker logs -f client_application",
    "docker:rebuild": "docker-compose down && docker-compose up --build -d",
    "docker:clean": "docker system prune -f"
  },
  "lint-staged": {
    "src/**/*.{ts,tsx}": [
      "prettier --write",
      "eslint --fix"
    ],
    "src/**/*.{md,json}": [
      "prettier --write"
    ]
  },
